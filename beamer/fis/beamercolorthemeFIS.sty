\mode<presentation>

% Gestion des couleurs 
\definecolor{fisfg}{rgb}{0.23,0.23,0.45}
\definecolor{fond}{rgb}{0.23,0.23,0.45}
\definecolor{llgray}{rgb}{.89,.83,1}
\definecolor{lgray}{rgb}{.84,.78,.97}
\definecolor{dgray}{rgb}{.47,.42,.59}

\setbeamercolor{structure}{fg=white,bg=fond}
\setbeamercolor{titlelike}{use=structure} %,fg=fis,bg=fond}
\setbeamercolor{title}{use=structure}%,fg=fis,bg=lgray}
\setbeamercolor{block title}{use=structure}%{fg=noir,bg=dgray}
\setbeamercolor{block title alerted}{use=alerted text}
\setbeamercolor{block title example}{use=example text}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=lgray}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=lgray}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=lgray}
\setbeamercolor{barhead}{fg=llgray,bg=fond}
\setbeamercolor{normal text}{fg=llgray,bg=fond}
\setbeamercolor{alerted text}{fg=white,bg=fond}
\setbeamercolor{background canvas}{parent=normal text}
\setbeamercolor{background}{parent=background canvas}

\mode
<all>
